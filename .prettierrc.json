{
	"arrowParens": "avoid",
	"tabWidth": 4,
	"trailingComma": "es5",
	"useTabs": true,
	"plugins": ["prettier-plugin-astro", "prettier-plugin-svelte"],
	"overrides": [
		{
			"files": "*.astro",
			"options": {
				"parser": "astro"
			}
		},
		{
			"files": "*.svelte",
			"options": {
				"parser": "svelte"
			}
		}
	]
}
